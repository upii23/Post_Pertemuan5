<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">
    <!-- Warna latar (bisa diganti sesuai tema) -->
    <solid android:color="#E0E0E0" />
    <!-- Garis tepi -->
    <stroke
        android:width="2dp"
        android:color="#FF9800" />
    <!-- Ukuran padding opsional -->
    <padding
        android:left="2dp"
        android:top="2dp"
        android:right="2dp"
        android:bottom="2dp" />
</shape>
